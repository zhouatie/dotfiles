            "key_code": "right_command",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "command pressed",
                "value": 1
              }
            },
            {
              "key_code": "right_command"
            }
          ],
          "to_delayed_action": {
            "to_if_canceled": [
              {
                "set_variable": {
                  "name": "command pressed",
                  "value": 0
                }
